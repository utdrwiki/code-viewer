        if (rooms[i].room_index == room_index)
        {
            room_id = rooms[i].room_id;
            break;
        }
    }
    if (room_id == room_gms_debug_failsafe)
        show_debug_message("*** ERROR: could not find room with index " + string(room_index));
    return room_id;
}

