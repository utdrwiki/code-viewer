function scr_get_room_by_id(arg0)
{
    var room_id = arg0;
    var rooms = scr_get_room_list();
    if (room_id < 10000)
        room_id += (global.chapter * 10000);
    var room_index = 0;
    for (var i = 0; i < array_length_1d(rooms); i++)
    {
        if (rooms[i].room_id == room_id)
        {
            room_index = rooms[i].room_index;
            break;
        }
    }
    return room_index;
}

